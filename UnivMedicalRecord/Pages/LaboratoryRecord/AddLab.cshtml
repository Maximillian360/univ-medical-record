                        <thead class="thead-dark">
                        <tr>
                            <th scope="col">GROSS EXAMINATION</th>
                            <th scope="col">RESULT</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <td>Color</td>
                            <td>
                                <input asp-for="StoolColor" type="text" class="form-control"/>
                            </td>
                        </tr>

                        <tr>
                            <td>Consistency</td>
                            <td>
                                <input asp-for="StoolConsistency" type="text" class="form-control"/>
                            </td>
                        </tr>

                        <tr>
                            <td>Other findings</td>
                            <td>
                                <input asp-for="OtherFindings" type="text" class="form-control"/>
                            </td>
                        </tr>

                        <tr>
                            <td>Occult Blood</td>
                            <td>
                                <input asp-for="OccultBlood" type="text" class="form-control"/>
                            </td>
                        </tr>
                        </tbody>
                        <br/>
                        <thead class="thead-dark">
                        <tr>
                            <th scope="col">MICROSCOPIC EXAMINATION</th>
                            <th scope="col">RESULT</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <td>Pus cells/HPF*</td>
                            <td>
                                <input asp-for="StoolPus" type="number" min="0" class="form-control"/>
                            </td>
                        </tr>

                        <tr>
                            <td>RBC/HPF*</td>
                            <td>
                                <input asp-for="StoolRbc" type="number" min="0" class="form-control"/>
                            </td>
                        </tr>

                        <tr>
                            <td>Other findings</td>
                            <td>
                                <input asp-for="MicroOtherFindings" type="text" class="form-control"/>
                            </td>
                        </tr>
                        <br/>
                        <tr>
                            <td>Remarks</td>
                            <td>
                                <input asp-for="MicroRemarks" type="text" class="form-control"/>
                            </td>
                        </tr>
                        </tbody>
                    </table>
                    <i>Remarks:</i>
                    <input asp-for="StoolRemarks" id="txtStoolRemarks" type="text" class="form-control" style="width: 100%;">
                    <br/>
                    <h2>OTHERS</h2>
                    <i>Remarks:</i>
                    <input asp-for="FurtherRemarks" id="txtFurtherRemarks" type="text" class="form-control" style="width: 100%;">
                </form>
            </div>
        }
    </div>
</div>

<input type="radio" name="tabs" id="tab3"/>
<label for="tab3"><span class="material-icons"></span>Cholesterol</label>

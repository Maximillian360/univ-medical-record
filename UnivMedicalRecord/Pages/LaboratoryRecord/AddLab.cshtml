                        </tr>

                        <tr>
                            <td>Remarks</td>
                            <td colspan="4">
                                <input asp-for="Remarks" type="text" class="form-control" style="width: 100%;"/>
                            </td>
                        </tr>

                        <tr>
                            <td>Other Remarks</td>
                            <td colspan="4">
                                <input asp-for="OtherRemarks" type="text" class="form-control" style="width: 100%;"/>
                            </td>
                        </tr>

                        </tbody>
                    </table>
                </form>
            </div>
        }
    </div>
</div>


